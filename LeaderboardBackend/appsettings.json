{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"EnvPath": ".env",
	"AllowedHosts": "*",
	"Brevo": {
		"SenderName": "Leaderboards.gg",
		"SenderEmail": "no-reply@leaderboards.gg"
	},
  "Feature": {
    "AccountRecovery": true,
    "AccountRegistration": true,
    "Login": true
  },
  "Limits": {
    "Default": {
      "Default": 64,
      "Max": 1024
    }
  }
}
